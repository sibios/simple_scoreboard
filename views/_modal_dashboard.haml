%div{ :class => "modal fade", :id => "dashboardModal", :tabindex => "-1", :role => "dialog", "aria-labelledby" => "flagModalLabel", "aria-hidden" => "true" }
  %div{ :class => "modal-dialog" }
    %div{ :class => "modal-content" }
      %div{ :class => "modal-header" }
        %button{ :type => "button", :class => "close", "data-dismiss" => "modal", "aria-label" => "Close" }<
          %span{ "aria-hidden" => "true" } &times;
        %h4{ :class => "modal-title" } Standings
      %div{ :class => "modal-body" }
        = haml :_dashboard, :locals => { :teams => teams }
      %div{ :class => "modal-footer" }
        %button{ :type => "button", :class => "btn btn-defaul", "data-dismiss" => "modal" } Close
